{
  "name": "zeroesones-dev/laravel-live-streaming",
  "description": "A Laravel package for live streaming using WebRTC and Socket.io.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Jahidul Islam Jahid",
      "email": "jsjahidmini@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "DreamersDesire\\LaravelLiveStreaming\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DreamersDesire\\LaravelLiveStreaming\\LiveStreamServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "version": "1.0.1"
}

